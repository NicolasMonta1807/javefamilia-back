<?xml version="1.0" encoding="UTF-8"?>
<persistence version="3.0" xmlns="https://jakarta.ee/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd">
    <persistence-unit name="reservas" transaction-type="JTA">
        <jta-data-source>java:/jdbc/JavefamiliaDS</jta-data-source>
        <class>com.javefamilia.gestionreservas.Model.Espacio</class>
        <class>com.javefamilia.gestionreservas.Model.HorarioEspacio</class>
        <class>com.javefamilia.gestionreservas.Model.Reserva</class>
        <properties>
            <property name="eclipselink.logging.level" value="FINE"/>
        </properties>
    </persistence-unit>
</persistence>
